{
  "name": "@recognizerhd/vue2vis",
  "version": "0.1.0",
  "description": "vue2vis monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "serve:graph2d": "lerna run serve --stream --scope @vue2vis/graph2d",
    "serve:network": "lerna run serve --stream --scope @vue2vis/network",
    "serve:timeline": "lerna run serve --stream --scope @vue2vis/timeline",
    "clean": "lerna exec -- rm -rf ./node_modules ./dist && rm -R ./node_modules",
    "commit": "yarn git-cz",
    "prepare": "lerna run prepare",
    "lerna:publish": "lerna publish --conventional-commits",
    "lerna:changed": "lerna changed",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn test:unit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/recognizerhd/vue2vis.git"
  },
  "contributors": [
    "Alex Couturon <infocontact.alex@gmail.com>",
    "Paul Warren <paulwarren.php@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "vue",
    "vis",
    "visjs"
  ],
  "dependencies": {
    "core-js": "^3.9.1",
    "vis-data": "^7.1.2",
    "vis-util": "^5.0.2"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@vue/cli-plugin-babel": "~4.5.11",
    "@vue/cli-plugin-eslint": "~4.5.11",
    "@vue/cli-plugin-unit-jest": "^3.12.1",
    "@vue/cli-service": "^3.3.1",
    "@vue/component-compiler": "^3.6.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.1.3",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.3",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "husky": "^5.1.3",
    "lerna": "^4.0.0",
    "moment": "^2.29.1",
    "prettier": "^2.2.1",
    "rollup": "^2.41.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "bugs": {
    "url": "https://github.com/recognizerHD/vue2vis/issues"
  },
  "homepage": "https://github.com/recognizerHD/vue2vis#readme",
  "main": ".eslintrc.js"
}
